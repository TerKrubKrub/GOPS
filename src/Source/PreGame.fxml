<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Source.Controller">
   <children>
      <ImageView fitHeight="700.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Resource/bg.png" />
         </image>
      </ImageView>
      <TextField alignment="CENTER" layoutX="295.0" layoutY="245.0" prefHeight="51.0" prefWidth="409.0">
         <font>
            <Font size="28.0" />
         </font>
      </TextField>
      <Label alignment="CENTER" layoutX="314.0" layoutY="175.0" text="Enter Your Name" textAlignment="CENTER">
         <font>
            <Font name="Broadway" size="40.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="botSelection" layoutX="408.0" layoutY="360.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="20.0" prefWidth="184.0" />
      <Button fx:id="preGameDoneButton" layoutX="435.0" layoutY="484.0" mnemonicParsing="false" onAction="#pressPreGameDone" prefHeight="50.0" prefWidth="129.0" text="DONE">
         <font>
            <Font name="Britannic Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
