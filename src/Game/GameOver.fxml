<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Menu.Controller">
    <children>
        <ImageView fitHeight="700.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Resource/Background/over.png" />
            </image>
        </ImageView>
        <Button fx:id="minimizeBtn" layoutX="912.0" layoutY="16.0" mnemonicParsing="false" onAction="#pressMinimizeBtn" prefWidth="25.0" rotate="15.0" stylesheets="@../File/Style.css" />
        <Button fx:id="closeBtn" layoutX="952.0" layoutY="16.0" mnemonicParsing="false" onAction="#pressCloseBtn" prefHeight="25.0" prefWidth="25.0" rotate="15.0" stylesheets="@../File/Style.css" />
        <Button fx:id="backBtn" layoutX="808.0" layoutY="588.0" mnemonicParsing="false" onAction="#pressBackBtn" prefHeight="50.0" prefWidth="129.0" stylesheets="@../File/Style.css" text="Back" textFill="WHITE">
            <font>
                <Font name="Arial Rounded MT Bold" size="20.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
